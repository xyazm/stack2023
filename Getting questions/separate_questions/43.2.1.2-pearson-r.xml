&lt;td&gt;\({@IQ6^2@}\)&lt;/td&gt;
&lt;td&gt;\({@ES6^2@}\)&lt;/td&gt;
&lt;/tr&gt;
&lt;tr&gt;
&lt;td&gt;\({@IQ7@}\)&lt;/td&gt;
