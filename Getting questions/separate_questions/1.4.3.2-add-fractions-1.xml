exp3: (v+1)/v+c1/(v+2);
ta3: ev( expand((v+1)*(v+2)+c1*v), simp)/(v*(v+2));

d1: ev(rand(5)+2,simp);
d2: ev(rand(4)+2, simp);
exp4: v/(v+1)- d1/(v+d2);
num4: ev(expand(v*(v+d2))-expand(d1*(v+1)),simp);
ta4: num4/((v+1)*(v+d2));</questionvariables>
