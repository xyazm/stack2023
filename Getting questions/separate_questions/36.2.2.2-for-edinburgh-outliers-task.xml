lo: rand([146.1, 146.2]);
hi: rand([189.4,189.5,189.7])
L: sort(random_beta(2, 10, 30));
L: lo + (hi-lo)*(L-L[1])/(L[30]-L[1]);
L: decimalplaces(float(L),1);

ta1:decimalplaces(float(L[1]),2);
ta2:decimalplaces(float(L[8]),2);
ta4:decimalplaces(float((L[15]+L[16])/2),2);
ta5:decimalplaces(float(L[23]-L[8]),2);
ta6:decimalplaces(float(L[23]),2);
ta8:decimalplaces(float(L[30]),2);
ta9:[["Not symmetrical", true], ["Symmetrical", false]];


/*for boxplot in solutions*/
mx: L[30]
qu: L[23]
mn: L[1]
ql: L[8]
CenterLine: if x&gt;mx then 5000 else if (x&lt;qu and x&gt;ql) then 5000 else if x&lt;mn then 5000 else 1;
qu: L[23]
ql: L[8]
upboxLine: if x&gt;qu then 5000 else if x&lt;ql then 5000 else 1+0.5*min(1,2*5*(x-ql)-1);
qu: L[23]
ql: L[8]
loboxLine: if x&gt;(qu+1/5) then 5000 else if x&lt;ql then 5000 else 1+0.5*max(-1,2*5*(x-qu)-1);
med: (L[15]+L[16])/2
vertLine: if x&lt;med-3 then 5000 else if x&gt;med+3 then 5000 else 1+0.5*max(-1,min(1,2*5*(x-med)-1));</questionvariables>
