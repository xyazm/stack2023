            <stackversion>2020120600</stackversion>
            <questionvariables>/*Randomization constants*/
const1: rand([15,17,19]); /*Series upper bound*/
const2: rand([3,4,5]); /*Lower bound series (b)*/

/*Series (a)*/
ser1: lambda([p], 3^(ev(p-1,simp:true))/(factorial(p)*(ev(const1+1-p,simp:true))));
terms1: [1,2,3,const1];
ta1: map(ser1,terms1);

/*Series (b)*/
ser2: lambda([p], (-p)^(ev(p-1,simp:true))/(p*(ev(p+2,simp:true))));
simp:true;
terms2: [const2,const2+1,const2+2,const1];
simp:false;
ta2: map(ser2,terms2);
</questionvariables>
