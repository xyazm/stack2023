            <stackversion>2020120600</stackversion>
            <questionvariables>/* Coefficient randomisation */
aa: rand_with_prohib(-3, 3, [0]);
bb: rand_with_prohib(-3, 3, [0]);

/* Equation */
f: aa + bb * x;
deq: 'diff(y, x, 2) + aa * 'diff(y, x) + bb * y = f;

/* Needed for solution */
pitest: a + b * x;
dpitest: diff(pitest, x);
ddpitest: diff(dpitest, x);
pieq: ddpitest + aa * dpitest + bb * pitest = f;
pieq1: aa * b + bb * a = aa;
pieq2: bb *  b = bb;
pieqsol: linsolve([pieq1, pieq2], [a, b]);
aval: rhs(pieqsol[1]);
bval: rhs(pieqsol[2]);
pi1: x;

ta1: x;</questionvariables>
