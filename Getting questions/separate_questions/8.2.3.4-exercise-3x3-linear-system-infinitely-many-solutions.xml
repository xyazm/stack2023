            <questionvariables>/* Set an order in the variables */
ordergreat(x, y, z, t);
/* Generate non-zero random numbers. They will be the coefficients of the first and second equations */
/* For display purposes, a1, a2 are positive, b1, b2 any, c1, c2 negative */
a1 : rand(6) + 1;
b1 : rand(6) * 2 - 5  ;
c1 : rand(6)  - 6;
d1 : rand(6) * 2 - 5
a2 : rand(6) + 1;
b2 : rand(6) * 2 - 5  ;
c2 : rand(6)  - 6;
d2 : rand(6) * 2 - 5
/* Generate a positive random number different than 0, 1. It will be the coefficient by which we multiply the third equation*/
/* For display purposes, it is positive */
kk : rand(3) + 1  ;
/* Define equations */
a3 : a1 + kk * a2;
b3 : b1 + kk * b2;
c3 : c1 + kk * c2;
d3 : d1 + kk * d2;
eqq1 : a1 * x + b1 * y + c1 * z = d1;
eqq2 : a2 * x + b2 * y + c2 * z = d2;
eqq3: a3 * x + b3 * y + c3 * z = d3;

AA : matrix([a1, b1, c1], [a2, b2, c2], [a3, b3, c3]);
BB : matrix([d1], [d2], [d3]);
detAA : determinant(AA);

/* Variables for dropdown list */
ta3:[["No solution",false],["One unique solution",false],["Infinitely many solutions",true]];
ta3:random_permutation(ta3);
ta3c: "Infinitely many solutions";

/* Determine solution to the system */
eq1 : a1 * x + b1 * y + c1 * t = d1;
eq2 : a2 * x + b2 * y + c2 * t = d2;
eq3: a3 * x + b3 * y + c3 * t = d3;
ta4 : algsys([eq1, eq2, eq3], [x, y]);
xx1 : rhs(ta4[1][1]);
xx2 : rhs(ta4[1][2]);
xx3 : t;
XX : matrix([xx1], [xx2], [xx3]);

nullMat : matrix([null], [null]);
</questionvariables>
