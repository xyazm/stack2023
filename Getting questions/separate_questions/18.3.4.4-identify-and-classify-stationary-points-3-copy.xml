            <stackversion>2021120900</stackversion>
            <questionvariables>/* This function has 1 saddle point */

ordergreat(C,x,y)
a1: rand([3,4,5]);     /*only these values for D&gt;0*/
C: rand([4,9,16, 25]);

exp: -x^2-a1*x*y-y^2+C;

partialx: diff(exp, x);
partialy: diff(exp, y);
a:solve(partialx = 0, x);
b:solve(partialy = 0, y);
tacoords1:matrix([0, 0]);
ta1: [["local minimum", false], ["local maximum", false], ["saddle point", true]];

/* Calclulate second derivatives and D*/
f_xx: diff(partialx,x);                              
f_yy: expand(diff(partialy,y));
f_xy: diff(partialy,x);
D: f_xx*f_yy-(f_xy)^2;

</questionvariables>
