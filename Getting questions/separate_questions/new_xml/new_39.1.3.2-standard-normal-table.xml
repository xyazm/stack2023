ex1: decimalplaces(float(num1),1);
ex2: decimalplaces(float(num1+num2*0.1),1);
ex3: decimalplaces(float(num1+num2*0.11),2);
ex4: decimalplaces(float(num1+num2*0.111),3);
ta1:decimalplaces(float(cdf_normal(ex1,0,1)-0.5),4);
ta2:decimalplaces(float(cdf_normal(ex2,0,1)-0.5),4);
ta3:decimalplaces(float(cdf_normal(ex3,0,1)-0.5),4);
EXACTta4: decimalplaces(float(cdf_normal(ex4,0,1)-0.5),4);
ex4b: ex3+0.01;
ta4b: decimalplaces(float(cdf_normal(ex4b,0,1)-0.5),4);
stp: num2*0.1;
ta4:decimalplaces(float(ta3+(stp*(ta4b-ta3))),4);</text>
