    <questionvariables>
				<text>a1: 1+rand(9);
a2: 2+rand(8);
aa: a2*x + a1;
Ansa: (x - a1)/a2;

b1: rand([-1,1]);
bb: b1*x;
Ansb: (1/b1)*x;

c1: 21+rand(20);
cc: -b1*c1*x;
Ansc: -(1/(b1*c1))*x;

d1: 1+rand(4);
d2: 1+rand(4);
dd: d1/(x + d2);
Ansd: (d1 - d2*x)/x;</text>
		</questionvariables>
