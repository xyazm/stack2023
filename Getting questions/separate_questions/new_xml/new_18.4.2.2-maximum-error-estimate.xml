    <questionvariables>
				<text>a: rand(3)+1;
b: rand(3)+1;
x_coord: rand([1,2,3]);
y_coord: rand_with_prohib(1,3,[x_coord]);
f: (a*x^2)+(y^2) + b*x*y;
error_x: rand([0.01, 0.02, 0.03]);
error_y: rand([0.04, 0.05, 0.06]);
f_x: diff(f, x);
f_y: diff(f, y);
f_eval: ev( f, x:x_coord, y:y_coord);  /* evaluate f at point */
f_x_eval: ev( f_x, x:x_coord, y:y_coord);  /* evaluate f_x at point */
f_y_eval: ev( f_y, x:x_coord, y:y_coord);  /*evaluate f_y at point */
delta_f: significantfigures((abs(f_x_eval)*error_x)+(abs(f_y_eval)*error_y),2);</text>
		</questionvariables>
