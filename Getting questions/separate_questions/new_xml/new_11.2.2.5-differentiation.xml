    <questionvariables>
				<text>a1:rand([3,4,5]);
f1: (a1 + e^t)^2
ta1:diff(expand(f1),t);

b1:rand([2,3,4]);
f2: pi*e^(2*s) + 1/s + b1*sin(pi*s)
ta2: diff( f2,s);

c1:rand([4,6,8]);
c2:c1/2;
f3: (1 + 1/r)^2 + (r+1)^2
ta3:diff(expand(f3) ,r);

d1:rand([2,4]);
d2:rand([3,5]);
f4: d1*sin(2*theta) - d2*cos(theta/4) + d2*theta^2
ta4:diff( f4, theta);

e1:rand([4,5,6]);
e2:e1-1;
e3:e1-2;
f5: e1*tan(3*t) + e2*sin(2*t) - e3*cos(4*t);
ta5:diff( f5 ,t);

</text>
		</questionvariables>
