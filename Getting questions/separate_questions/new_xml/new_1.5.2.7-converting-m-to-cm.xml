    <questionvariables>
				<text>stack_unit_si_declare(true);
eqn: x*100;
a1: rand([5,10,15]);
au1:stackunits(dispsf(float(a1),2), m);
a2: rand([15/100,33/100,25/100]);
au2:stackunits(dispsf(float(a2),2), m);
a3: rand([562/10,162/10,282/10]);
au3:stackunits(dispsf(float(a3),3), m);

ta1: stackunits(dispsf(float(ev(eqn, x=a1)),2), cm);
ta2: stackunits(dispsf(float(ev(eqn, x=a2)),2), cm);
ta3: stackunits(dispsf(float(ev(eqn, x=a3)),3), cm);</text>
