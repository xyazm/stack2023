    <questionvariables>
				<text>L:makelist(i,i,1,15);
L:sort(rand_selection(L,10));
sum:lsum(i,i,L);
m:mean(L);
sum2:lsum(i^2,i,L)-length(L)*m^2;
ta1:decimalplaces(float(var1(L)),3);
ta2:decimalplaces(float(sqrt(ta1)),3);
</text>
