    <questionvariables>
				<text>n: rand_with_step(2, 5, 1);
k: rand_with_step(2, 4, 1);
exp1: A * cos(n * x) + B * sin(n * x);
exp2: A * e^(-k * t);
exp3: A * sin(x) + B * sinh(x) + C * cos(x) + D * cosh(x);

d1exp1: diff(exp1, x);
d2exp1: diff(d1exp1, x);
ta1: -n^2 * y;

d1exp2: diff(exp2, t);
ta2: -k * x;

d1exp3: diff(exp3, x);
d2exp3: diff(d1exp3, x);
d3exp3: diff(d2exp3, x);
d4exp3: diff(d3exp3, x);
ta3: y;</text>
		</questionvariables>
