				<text>exp: (x^2 + y^2)^2 - 2*(x^2 - y^2) + 1;
partialx: diff(exp, x);
partialy: diff(exp, y);
f_xx: diff(partialx, x);
f_yy: diff(partialy, y);
f_xy: diff(partialx,y);
D: expand(f_xx*f_yy-(f_xy)^2);

tacoords1:matrix([0, 0]); 
ta1: [["local minimum", false], ["local maximum", false], ["saddle point", true]];
tacoords2:matrix([1, 0]); 
ta2: [["local minimum", true], ["local maximum", false], ["saddle point", false]];
tacoords3:matrix([-1,0]); 
ta3: [["local minimum", true], ["local maximum", false], ["saddle point", false]];

/* Evaluate second derivatives and D */
fxxval1: ev(f_xx, x=0, y=0);
fxxval2: ev(f_xx, x=1, y=0);
fxxval3: ev(f_xx, x=-1, y=0);
fyyval1: ev( f_yy, x= 0, y=0);
fyyval2: ev( f_yy, x= 1, y=0);
fyyval3: ev( f_yy, x= -1, y=0);
fxyval1: ev( f_xy, x=0, y=0);
fxyval2: ev( f_xy, x=1, y=0);
fxyval3: ev( f_xy, x=-1, y=0);
Dval1: ev(D, x= 0, y=0);
Dval2: ev(D, x=1, y=0);
Dval3: ev(D, x=-1, y=0);</text>
		</questionvariables>
