L:[155.3,177.3,146.2+rand([0,5]),163.1-rand([0,5]),161.8,146.3+rand([0,5]),
167.9,165.4,172.3,188.2,178.8,151.1+rand([0,5]),189.4,164.9+rand([0,5,-5]), 174.8,160.2,187.1,163.2-rand([0,5]),147.1,182.2,178.2,172.8,
164.4+rand([0,5,-5]),177.8,154.6,154.9,176.3,148.5,161.8,178.4];
l:length(L); 
ta1:block(s:0, for i:1 thru l do if (L[i] < 150) then s:s+1, return(s));
ta2:block(s:0, for i:1 thru l do if (L[i] < 155) then s:s+1, return(s))-ta1;
ta3:block(s:0, for i:1 thru l do if (L[i] < 160) then s:s+1, return(s))-ta1-ta2;
ta4:block(s:0, for i:1 thru l do if (L[i] < 165) then s:s+1, return(s))-ta1-ta2-ta3;
ta5:block(s:0, for i:1 thru l do if (L[i] < 170) then s:s+1, return(s))-ta1-ta2-ta3-ta4;
ta6:3;
ta7:6;
ta8:1;
ta9:3;

/*  hist: histogram (
           L,
           nclasses     = [145,190,9],
           xlabel       = "Height (cm)",
           ylabel       = "Frequency",
           fill_color   = grey,
           fill_density = 0.6); */
</text>
