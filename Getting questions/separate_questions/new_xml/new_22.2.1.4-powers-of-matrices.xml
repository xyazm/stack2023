    <questionvariables>
				<text>simp:true
/*Generate the eigenvalues and the third power of D*/
eval1:rand_with_prohib(-3,3,[0,1,-1])
eval2:rand_with_prohib(-3,3,[0,1])
matD:matrix([eval1,0],[0,eval2])
power:rand(7)+8

matD2:matD.matD
matD3:matD2.matD
matDn:matrix([eval1^(power),0],[0,eval2^(power)])
simp:false</text>
		</questionvariables>
