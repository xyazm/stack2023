    <questionvariables>
				<text>f1: rand([1,2,3])+t;
a1: 0;
b1: 2;
ta1: sqrt(1/(b1-a1)*integrate(f1^2,t,a1,b1));
ta1: decimalplaces(float(ta1),4);

f2: 2*x+rand([1,-1]);
a2: -1;
b2: 1;
ta2: sqrt(1/(b2-a2)*integrate(f2^2,x,a2,b2));
ta2: decimalplaces(float(ta2),4);

f3: t^2;
a3: 0;
b3: rand([1,2]);
ta3: sqrt(1/(b3-a3)*integrate(f3^2,t,a3,b3));
ta3: decimalplaces(float(ta3),4);

f4: t^2;
a4: 0;
b4: b3+1;
ta4: sqrt(1/(b4-a4)*integrate(f4^2,t,a4,b4));
ta4: decimalplaces(float(ta4),4);

f5: z^2+z;
a5: rand([1,2,3]);
b5: a5+2;
ta5: sqrt(1/(b5-a5)*integrate(f5^2,z,a5,b5));
ta5: decimalplaces(float(ta5),4);</text>
