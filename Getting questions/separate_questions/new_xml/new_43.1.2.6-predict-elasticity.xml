				<text>/* this contains the pearson coefficient question (ex06q2) */
moe : [9.5,9,11,11.5,4.5,6,13.5,6.5,8,5.5,15,12.5,15.5,8.5,10.5]
mor : [47.5,53,59,66.5,34.5,43,72.5,41.5,51,31.5,79,63.5,88.5,44.5,64.5]

/*moe : makelist(float((10+rand(22))/2),15)
mor : makelist(float(ceiling(2*(10.339+4.659*moe[i])-3+(rand(60)/10))/2),i,15)*/

ta1 : moe[15]^2
ta2 : moe[15]*mor[15]
ta3 : mor[15]^2
totalmoe : 0 ; totalmor : 0 ; totalmoe2 : 0 ; totalmoemor : 0 ; totalmor2 : 0
dum1 : for i:1 thru 15 do totalmoe : totalmoe+moe[i]
dum2 : for i:1 thru 15 do totalmor : totalmor +mor[i]
dum3 : for i:1 thru 15 do totalmoe2 : totalmoe2 + moe[i]*moe[i]
dum4 : for i:1 thru 15 do totalmoemor : totalmoemor + moe[i]*mor[i]
dum5 : for i:1 thru 15 do totalmor2 : totalmor2 + mor[i]*mor[i]
ta4 : totalmoe
ta5 : totalmor
ta6 : totalmoe2
ta7 : totalmoemor
ta8 : totalmor2
slope : ((15*totalmoemor)-(totalmoe*totalmor))/((15*totalmoe2)-totalmoe^2)
ta9 : significantfigures(slope,3)
intercept : (totalmor/15)-slope*(totalmoe/15)
ta10 : significantfigures(intercept, 3)
var : 6+rand(6)
ta11 : significantfigures(intercept+var*slope,3)
alternateta11 : significantfigures(ta9+var*ta10,3)

/* I made it so that if they use the intercept and slope rounded to 3 sf to calculate their answer for part (b) they still get full marks. This can be changed by deleting the second node in prt11. They still need to have entered their answer to 3 significant figures though. */

unroundedta12 : (15*totalmoemor-totalmoe*totalmor)/sqrt((15*totalmoe2-totalmoe^2)*(15*totalmor2-totalmor^2)) 
ta12 : significantfigures(unroundedta12,3)</text>
