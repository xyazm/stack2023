    <questionvariables>
				<text>x1:30;    x5:50;    x9:70;
x2:40;    x6:50;   x10:80;
x3:40;    x7:60;   x11:80;
x4:50;    x8:70;    x12:80;

y1:70+rand(4);    y5:130+rand(4);    y9:200-rand(4);
y2:90+rand(4);    y6:150;   y10:200
y3:100-rand(4);    y7:160+rand(4);  y11:220-rand(4);
y4:120-rand(4);    y8:190;    y12:230+rand(4);

n:12;

sumX:x1+x2+x3+x4+x5+x6+x7+x8+x9+x10+x11+x12;
sumX2:x1^2+x2^2+x3^2+x4^2+x5^2+x6^2+x7^2+x8^2+x9^2+x10^2+x11^2+x12^2;
sumXY:x1*y1 + x2*y2 + x3*y3 + x4*y4 + x5*y5 + x6*y6 + x7*y7 + x8*y8 + x9*y9 + x10*y10 + x11*y11 + x12*y12;
sumY:y1+y2+y3+y4+y5+y6+y7+y8+y9+y10+y11+y12;

b:decimalplaces((sumXY/n - sumX/n*sumY/n)/(sumX2/n - (sumX/n)^2),3);
a:decimalplaces(1/n*(sumY - b*sumX),3);
ta1:b;
ta2:a;


</text>
