    <questionvariables>
				<text>a: rand(3)+1;
b: rand(3)+1;
c: rand(3)+1;
x_coord: rand([-3,-2,-1,1,2,3]);
y_coord: rand([-3,-2,-1,1,2,3]);

f: a*x^2*y^2 + b*x + c*y;
delta_x: rand([0.05,0.06,0.07,0.08,0.09]);
delta_y: rand([0.025, 0.035, 0.045, 0.055,0.065, 0.075, 0.085]);
f_x: diff(f, x);
f_y: diff(f,y);
f_x_eval: ev( f_x, x:x_coord, y:y_coord);  /* evaluate f_x at point */
f_y_eval: ev( f_y, x:x_coord, y:y_coord);  /*evaluate f_y at point */
delta_f: (f_x_eval*delta_x)+(f_y_eval*delta_y);
delta_f: significantfigures(delta_f,2);

/* For worked soution*/
f1: ev( f, x:x_coord+delta_x, y:y_coord+delta_y);
fp: ev( f, x:x_coord, y:y_coord);</text>
		</questionvariables>
