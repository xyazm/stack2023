    <questionvariables>
				<text>aa: rand([2, 4, 6, 8]); 
bb: rand([3, 5, 7, 9]); 
cc: rand(4)+1;

f1: x;
g1: cos(aa * x);
exp1: f1 * g1;

f2: x;
g2: sin(aa * x);
exp2: f2 * g2;

f3: t;
g3: e^(aa * t);
exp3: f3 * g3;

l1: 0;
u1: 1;
ta1: decimalplaces(float(int(exp1, x, l1, u1)),4);

l2: 0;
u2: pi/2; 
ta2r: int(exp2, x, l2, u2);
ta2: decimalplaces(float(int(exp2, x, l2, u2)),4); 

l3: -1;
u3: 1;
ta3r: int(exp3, t, l3, u3);
ta3: decimalplaces(float(int(exp3, t, l3, u3)),4);

/* Needed for solution */
df1: diff(f1, x);
ig1: int(g1, x);
fig1: f1 * ig1;
idfig1: int(df1 * ig1, x);
u11: subst(l1, x, fig1);
u12: subst(u1, x, fig1);
v11: subst(l1, x, idfig1);
v12: subst(u1, x, idfig1);

df2: diff(f2, x);
ig2: int(g2, x);
fig2: f2 * ig2;
idfig2: int(df2 * ig2, x);
u21: subst(l2, x, fig2);
u22: subst(u2, x, fig2);
v21: subst(l2, x, idfig2);
v22: subst(u2, x, idfig2);

df3: diff(f3, t);
ig3: int(g3, t);
fig3: f3 * ig3;
idfig3: int(df3 * ig3, t);
u31: subst(l3, t, fig3);
u32: subst(u3, t, fig3);
v31: subst(l3, t, idfig3);
v32: subst(u3, t, idfig3);</text>
		</questionvariables>
