    <questionvariables>
				<text>aa: rand([3, 5, 7, 9]); bb: rand([2, 4, 6, 8])
exp1: x^4+x*y^(aa)+((bb)*x^(aa))*y^2; 
ta1: diff(diff(exp1,x), x); ta2: diff(diff(exp1,y), y);
ta3: diff(diff(exp1,y), x);

x1: 1; x2: -2; x3: -1;
