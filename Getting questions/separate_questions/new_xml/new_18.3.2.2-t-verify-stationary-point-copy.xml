    <questionvariables>
				<text>ordergreat(x, y);

f: 8*x^2 + 6*y^2 - 2*y^3 + 5;
f_x: diff(f, x);
f_y: diff(f, y);

ta1: f_x;
ta2: f_y;
ta3: ev(f_x, x=0, y=2);
ta4: ev(f_y, x=0, y=2);
ta5: ev(f, x=0, y=2);</text>
