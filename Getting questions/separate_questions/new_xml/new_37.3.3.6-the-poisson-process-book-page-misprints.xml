    <questionvariables>
				<text>a:rand([750,300,450,900]);
lambda: a/150;
ta1: decimalplaces(float(pdf_poisson(0,lambda)),4);
ta2: decimalplaces(float(pdf_poisson(5,lambda)),4);
ta3: decimalplaces(float(cdf_poisson(2,lambda)),4);
ta4: decimalplaces(float(1 - cdf_poisson(1,lambda)),4);</text>
		</questionvariables>
