    <questionvariables>
				<text>a1:rand([0.1, 0.3, 0.4]);
f1: x^2 + sin(x);
df1: diff(f1,x);
ta1:decimalplaces(ev(df1,x:a1),3);

b1:rand([pi/3]);
f2: 4*sin(2*t) + 3*t;
df2: diff(f2,t);
ta2:ev(df2, t:b1);

c1:rand([1.3,1.5,1.7]);
f3: 5*sin(t) - 3*cos(2*t);
df3: diff(f3,t);
ta3:decimalplaces(ev(df3,t:c1),3);


</text>
