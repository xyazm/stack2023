    <questionvariables>
				<text>a11: rand([-4,-3,-2]);
a21: ev(a11+1, simp);
a31: ev(a11-1, simp);
a12:a22:a32: 0;
a13: rand([4,5]);
a23: ev(a13-3, simp);
a33: -a31;

eqs1: rand([2*x+1,3*x+1]);
eqs2: rand([-3*x+5,-4*x+6]);
eqs3: x^2;

ta1: ev(eqs1,x=a11, simp)
ta2: ev(eqs1,x=a12, simp)
ta3: ev(eqs1,x=a13, simp)
ta4: ev(eqs2,x=a21, simp)
ta5: ev(eqs2,x=a22, simp)
ta6: ev(eqs2,x=a23, simp)
ta7: ev(eqs3,x=a31, simp)
ta8: ev(eqs3,x=a32, simp)
ta9: ev(eqs3,x=a33, simp)

simp: false;
test: subst(a11, x, eqs1);
</text>
		</questionvariables>
