    <questionvariables>
				<text>n: 2 + rand(5);
exp: A * cosh(n * x) + B * sinh(n * x);
dy: diff(exp, x);
d2y: diff(dy, x);
ra: n^2 * y;
wa1: n * y;
wa2: n^2 * y^2;
wa3: -n^2 * y;
ta: [[ra, true], [wa1, false], [wa2, false], [wa3, false]];
ta: random_permutation(ta);</text>
