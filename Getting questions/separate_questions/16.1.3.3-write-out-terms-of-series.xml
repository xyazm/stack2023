          <stackoptions id="16603">
            <stackversion>2020120600</stackversion>
            <questionvariables>/*Series definition*/

p_gap:rand(3)+1;      /*constant for denominator*/
ap: (-1)^p/(p+p_gap)^2;    /*typical term*/
p1: rand(3);               /*first term index*/
n: p1 + 4;                  /*upper bound*/

/*model answers*/
ta1: subst(p1,p,ap);
ta2: subst(p1+1,p,ap);
ta3: subst(p1+2,p,ap);
ta4: subst(p1+3,p,ap);
ta5: subst(n,p,ap);</questionvariables>
