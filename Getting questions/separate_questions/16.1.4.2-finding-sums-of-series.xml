            <questionvariables>/*Arithmetic and geometric series sums*/
asum: lambda([a,d,n], n*(2*a+d*(n-1))/2);
gsum: lambda([a,r,n], a*(1-r^n)/(1-r));

/*Randomization constants*/
[aa,ad,an]: rand([[1,1,100],[2,2,100],[3,3,100]]);
[ba,br,bn]: rand([[1/2,1/3,6],[1/3,1/2,6]]);

/*Series 1 - arithmetic*/
an_a: aa+(n-1)*ad;    /*nth term of arithmetic progresion */
n_1:4;                /*the terms I want to diplay */
SA1:ev(makelist(k,k,1,n_1),simp);             /* create the sum of terms */
SA2:maplist(lambda([ex],ev(an_a,n=ex)),SA1);
SA3:append(ev(SA2,simp),["...",ev(an_a,n=an,simp)]);
SA4:apply("+",SA3);
ta1: ev(asum(aa,ad,an),simp);

/*Series 3 - geometric*/
an_c: ba*(br)^(n-1);          /*nth term of geogetric progresion */
SC1:ev(makelist(k,k,1,bn),simp);             /* create the sum of terms */
SC2:maplist(lambda([ex],ev(an_c,n=ex)),SC1);
SC3:ev(SC2,simp);
SC4:apply("+",SC3);
ta3: ev(gsum(ba,br,bn),simp);</questionvariables>
